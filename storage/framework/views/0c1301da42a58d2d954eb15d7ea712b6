			<a href="<?php echo e(route('songs_path', [$listsong->slug])); ?>" id="recent"><?php echo e($listsong->title); ?></span></a>
			<?php if($listsong->version > 1): ?>
				<span>v.<?php echo e($listsong->version); ?></span>
			<?php endif; ?>
			<span id="artist">
				<?php if($listsong->artist <> ""): ?>
					by:<?php echo e($listsong->artist); ?>

				<?php else: ?>
					by:Unknown
				<?php endif; ?>
			</span>
		</li>
	<?php endforeach; ?>
	<?php $count=0; ?>
